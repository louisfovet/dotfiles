dupf(){
  if [ $# -eq 1 ]; then
    cp $1 ${1}.backup
    vi $1
  else
    echo "Use only 1 arg (base file), dumbass !"
  fi
}

gitm(){
  if [ $# -eq 1 ]; then
    git add .
    git commit -m $1
    git push 
  else
    echo "Use only 1 arg (commit message), dumbass !"
  fi
}

dockerd(){
  if [ $# -eq 1 ]; then
    echo "\n##### BUILDING DOCKER IMAGE #####"
    docker build -t $1 .
    echo "\n##### RUNNING CONTAINER #####"
    docker run $1
  fi
}
